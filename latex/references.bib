@article{shenConstructionMultipleConcentration2023,
  title = {Construction of Multiple Concentration Gradients for Single-Cell Level Drug Screening},
  author = {Shen, Shaofei and Zhang, Fangjuan and Zhang, Yali and Li, Yi and Niu, Yanbing and Pang, Long and Wang, Jinyi},
